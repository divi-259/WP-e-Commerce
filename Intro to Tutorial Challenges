#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int v,n,a[1000],j;
    cin>>v>>n;
    for(int i=0;i<n;i++)
        {
        cin>>a[i];
        if(v==a[i])
            j=i;
    }
    cout<<j;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
